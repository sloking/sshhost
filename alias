sshh='ssh -t -t -i ~/.ssh/config/id_rsa_sshhost user@sshhost "bash /home/user/.ssh/_batch/dossh.sh"'
sshhost='ssh -i ~/.ssh/config/id_rsa_sshhost root@sshhost'
sshhostlog='ssh -i ~/.ssh/config/id_rsa_sshhost root@sshhost tail -f -n 100 /home/user/.ssh/log/sshconnect.log'
sshhostopenconn='ssh -t -t -i ~/.ssh/config/id_rsa_sshhost root@sshhost bash /home/user/.ssh/_batch/watch_dir.sh'
